#!/bin/bash

echo "^.^ ~( Running Program and Generating README.MD )"

./setup.sh |tee ./ai/logs/output.log

echo
echo "GENERATING \"README.md\" TEXT FILE."

cat ./addy/component_readme_1.txt > README.md
cat ./ai/logs/output.log >> README.md
cat ./addy/component_readme_1.txt >> README.md

echo "done."
exit 0
